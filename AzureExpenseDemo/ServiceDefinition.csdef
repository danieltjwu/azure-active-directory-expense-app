<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="AADExpenseDemo" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2012-05.1.7">
  <WebRole name="AADExpenseDemo" vmsize="Small">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint2" endpointName="HttpsIn" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="HttpsIn" protocol="https" port="8080" />
    </Endpoints>
    <Imports></Imports>
    <Certificates></Certificates>
    <ConfigurationSettings></ConfigurationSettings>
  </WebRole>
</ServiceDefinition>